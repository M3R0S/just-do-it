{
    "name": "just-do-it",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start:dev\" \"npm run start:server\"",
        "start:dev": "webpack serve --env PORT=3000",
        "start:server": "node ./server/index.js",
        "build:dev": "webpack --env MODE=development",
        "build:prod": "webpack --env MODE=production API_URL=https://just-do-it-server.vercel.app/",
        "analyze:dev": "webpack --env MODE=development ANALYZE=true",
        "analyze:prod": "webpack --env MODE=production ANALYZE=true",
        "lint:ts": "eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "lint:scss": "stylelint \"**/*.scss\"",
        "lint:scss:fix": "stylelint \"**/*.scss\" --fix",
        "format": "prettier --check \"**/*.{ts,tsx,scss,json}\"",
        "format:fix": "prettier --write \"**/*.{ts,tsx,scss,json}\"",
        "test:unit": "jest",
        "test:ui": "loki test",
        "test:ui:ok": "loki approve",
        "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
        "test:ui:report:json": "node scripts/generate-visual-json-report.js",
        "test:ui:report:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "test:ui:report": "npm-run-all test:ui:report:*",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "storybook:build": "build-storybook -c ./config/storybook",
        "prepare": "husky install"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.17.5",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-react": "^7.16.7",
        "@babel/preset-typescript": "^7.16.7",
        "@csstools/selector-specificity": "^2.0.2",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
        "@storybook/addon-actions": "^6.4.19",
        "@storybook/addon-essentials": "^6.4.19",
        "@storybook/addon-interactions": "^6.4.19",
        "@storybook/addon-links": "^6.4.19",
        "@storybook/builder-webpack5": "^6.4.19",
        "@storybook/manager-webpack5": "^6.4.19",
        "@storybook/react": "^6.4.19",
        "@storybook/testing-library": "^0.0.9",
        "@svgr/webpack": "^6.2.1",
        "@testing-library/jest-dom": "^5.16.2",
        "@testing-library/react": "^12.1.3",
        "@types/jest": "^29.4.0",
        "@types/node": "^17.0.21",
